and-or-not-rule[VIOLATION] :=  ∀x : ( Class(x,'apiContract:Operation') → ( ¬( In(x,'apiContract:method',[get]) ) ∨ ( ¬(  ∃y:>= 1 : ( Nested(x,y,'apiContract:returns') → ( Pattern(y,'apiContract:statusCode','^201$') ) ) ) ∧ (  ∃z:>= 1 : ( Nested(x,z,'apiContract:returns') → ( Pattern(z,'apiContract:statusCode','^2[0-9]{2}$') ) ) ) ∧ (  ∃p:>= 1 : ( Nested(x,p,'apiContract:returns') → ( Pattern(p,'apiContract:statusCode','^4[0-9]{2}$') ) ) ) ∧ (  ∃q:>= 1 : ( Nested(x,q,'apiContract:returns') → ( Pattern(q,'apiContract:statusCode','^5[0-9]{2}$') ) ) ) ) ) )