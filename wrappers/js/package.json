{
  "name": "@aml-org/amf-custom-validator",
  "version": "1.7.0-SNAPSHOT",
  "description": "AMF validator backed by OPA Rego",
  "main": "dist/bundle.js",
  "scripts": {
    "test:web": "npm run build:test:web && npm run test:web:only",
    "test:web:only": "cypress run --config-file ./test/browser/cypress.config.js",
    "test:node": "mocha -t 10000 --recursive './test/node/simple.js'",
    "test": "npm run build:dist && npm run test:node && npm run test:web",
    "test:only": "npm run test:node && npm run test:web",
    "build:test:web": "webpack --config ./test/browser/webpack.config.js",
    "build:dist": "rollup --config"
  },
  "browser": {
    "crypto": false,
    "fs": false,
    "util": false
  },
  "author": "Antonio Garrote",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-url": "^8.0.2",
    "buffer": "^6.0.3",
    "cypress": "^13.6.3",
    "mocha": "^10.2.0",
    "mocka": "^0.0.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "browser-or-node": "^3.0.0-pre.0",
    "pako": "^2.1.0"
  }
}
